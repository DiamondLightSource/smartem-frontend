{
  "name": "",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "dev:mock": "VITE_ENABLE_MOCKS=true vite",
    "preview": "vite preview",
    "typecheck": "tsc",
    "router:generate": "tsr generate",
    "router:check": "tsr generate && git diff --exit-code src/routeTree.gen.ts",
    "api:fetch": "curl -s https://diamondlightsource.github.io/smartem-decisions/main/api/smartem/swagger.json -o src/api/openapi.json",
    "api:fetch:local": "curl -s http://localhost:8000/openapi.json -o src/api/openapi.json",
    "api:generate": "orval --config orval.config.ts && node scripts/extract-api-version.js",
    "api:update": "npm run api:fetch && npm run api:generate",
    "prepare": "lefthook install",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "npm run format:biome && npm run format:prettier",
    "format:biome": "biome format --write .",
    "format:prettier": "prettier --write \"**/*.{yml,yaml,md}\"",
    "format:check": "biome format . && prettier --check \"**/*.{yml,yaml,md}\"",
    "check": "biome check .",
    "check:fix": "biome check --write ."
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@mui/x-charts": "^8.0.0",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-router": "^1.135.0",
    "@tanstack/router-devtools": "^1.135.0",
    "@tanstack/router-vite-plugin": "^1.135.0",
    "@types/d3-array": "^3.2.1",
    "@vitejs/plugin-react": "^5.1.0",
    "axios": "^1.12.2",
    "boring-avatars": "^1.11.2",
    "d3-array": "^3.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@faker-js/faker": "^10.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "lefthook": "^1.13.6",
    "msw": "^2.11.3",
    "orval": "^7.13.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2",
    "vite": "^7.1.11",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
