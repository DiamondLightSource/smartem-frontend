{
  "name": "",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "dev:mock": "VITE_ENABLE_MOCKS=true vite",
    "preview": "vite preview",
    "typecheck": "tsc",
    "router:generate": "tsr generate",
    "router:check": "tsr generate && git diff --exit-code src/routeTree.gen.ts",
    "api:fetch": "curl -s https://diamondlightsource.github.io/smartem-decisions/main/api/smartem/swagger.json -o src/api/openapi.json",
    "api:fetch:local": "curl -s http://localhost:8000/openapi.json -o src/api/openapi.json",
    "api:generate": "orval --config orval.config.ts && node scripts/extract-api-version.js",
    "api:update": "npm run api:fetch && npm run api:generate",
    "prepare": "lefthook install",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "npm run format:biome && npm run format:prettier",
    "format:biome": "biome format --write .",
    "format:prettier": "prettier --write \"**/*.{yml,yaml,md}\"",
    "format:check": "biome format . && prettier --check \"**/*.{yml,yaml,md}\"",
    "check": "biome check .",
    "check:fix": "biome check --write ."
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.0.2",
    "@mui/x-charts": "^8.25.0",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-router": "^1.153.2",
    "@tanstack/router-devtools": "^1.153.2",
    "@tanstack/router-vite-plugin": "^1.153.2",
    "@types/d3-array": "^3.2.1",
    "@vitejs/plugin-react": "^5.1.2",
    "axios": "^1.13.2",
    "boring-avatars": "^2.0.4",
    "d3-array": "^3.2.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@faker-js/faker": "^10.2.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/router-cli": "^1.153.2",
    "@types/node": "^25",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "lefthook": "^2.0.15",
    "msw": "^2.12.7",
    "orval": "^8.0.2",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.4"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "overrides": {
    "@asyncapi/specs": "6.10.0",
    "tar": "^7.5.2",
    "validator": "^13.15.22",
    "js-yaml": "^4.1.1"
  }
}
